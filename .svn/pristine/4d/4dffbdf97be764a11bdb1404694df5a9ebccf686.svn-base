.settingBigDiv{
	width: 1024px;
	height: auto;
	margin-top: 100px;
	padding: 30px;
}
.settingBigDiv .backLink{
	position: absolute;
	top: 25px;
	color: #000000;
}
.settingBigDiv .backLink .back_icon{
	display: inline-block;
	width: 30px;
	height: 30px;
	background: url(../../images/back_icon.png);
	background-size: 30px auto;
}
.settingBigDiv .settingDiv{	
	width: 500px;
	min-height: 650px;
	margin-top: 40px;
	padding: 3px;
	display: inline-block;
	text-align: center;
	float: left;
}
.settingBigDiv .settingDiv h3{
	margin-top: 15px;
}
.settingBigDiv .settingDiv .infoDiv{
	text-align: center;
}
.settingBigDiv .settingDiv .infoDiv .infoRow{
	width: 300px;
	min-height:80px;
	border-bottom: 2px solid rgba(0,0,0,0.5);
}
.settingBigDiv .settingDiv .infoDiv .infoRow .infoInput{
	max-width: 200px;
	min-height: 28px;
	background: #eeeeee;
	margin: 10px 0px 10px 15px;
	padding: 5px 10px;
	border-left:1px solid rgba(0,0,0,0.3);
}
.settingBigDiv .settingDiv .infoDiv .button{
	padding: 12px 15px;
	background: #111d6d;
	color: #ffffff;
	border-radius: 3px;
}

.settingBigDiv .settingDiv .factoryCard{
	width: 360px;
	height: 160px;
	padding: 20px;
	margin-left: 50px;
	margin-top: 20px;
	border:1px solid #111d6d;
	position: relative;
	border-radius: 3px;
}
.factoryCard:hover{
	box-shadow: 0px 0px 10px #888888;
}
.settingBigDiv .settingDiv .factoryCard .ooeSettingDiv{
	width: 100%;
	height: 60px;
	text-align: center;
}
.settingBigDiv .settingDiv .factoryCard .ooeSettingDiv .ooeTimeSetDiv{
	margin-top: 20px;
	font-size: 15px;
}
.settingBigDiv .settingDiv .factoryCard .ooeSettingDiv .timeClass{
	width: 50px;
	height: 25px;
	text-align: center;
	color: #888888;
}
.settingBigDiv .settingDiv .factoryCard .fDetails{
	position: absolute;
	left: 20px;
	bottom: 20px;
	font-size: 15px;
}
